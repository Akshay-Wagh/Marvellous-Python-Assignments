# Write a recursive program which display below pattern.
# Input : 5
# Output : * * * * *

i = 0
def Display(no):
    global i
    if i < no:
        print("*", end = "  ")
        i = i + 1
        Display(no)

def main():
    print("Enter number")
    value=int(input())

    Display(value)

if __name__=="__main__":
    main()
