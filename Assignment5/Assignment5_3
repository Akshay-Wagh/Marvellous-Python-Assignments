# Write a recursive program which display below pattern.
# Input : 5
# Output : 5  4  3  2  1

def Display(no):

    if no>0:
        print(no, end="   ")
        no = no - 1
        Display(no)

def main():
    print("Enter number")
    value=int(input())

    Display(value)

if __name__=="__main__":
    main()
