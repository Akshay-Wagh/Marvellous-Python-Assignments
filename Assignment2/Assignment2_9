# Write a program which accept number from user and return number of digits in that number.
# Input : 5187934
# Output : 7

def Digit(No):
    icnt=0
    while(No!=0):
        No=No//10
        icnt=icnt+1
    return icnt

def main():
    value=int(input("Enter Number"))
    ret=Digit(value)
    print("Digits in number are:",ret)

if __name__=="__main__":
    main()


